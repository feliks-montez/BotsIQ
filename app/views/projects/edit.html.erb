<h1>Edit Project</h1>
<%= form_for [@team, @project] do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  
  <div class="form-group">
    <%= f.label :deadline %>
    <%= f.date_select :deadline %>
  </div>
  
  <div class="form-group">
    <%= f.label :completed %>
    <%= f.check_box :completed %>
  </div>
  
  <%= f.submit "Update Project", class: 'btn btn-primary' %>
<% end %>
